{
  "name": "reguilded",
  "version": "0.0.3-alpha",
  "description": "ReGuilded is a client injector/client mod that allows you to extend the functionality of Guilded's client by providing theme and add-on support.",
  "main": "",
  "type": "module",
  "scripts": {
    "inject": "node src/inject/index.js inject",
    "uninject": "node src/inject/index.js uninject",
    "reinject": "npm run uninject && npm run inject",
    "build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReGuilded/ReGuilded.git"
  },
  "author": "ReGuilded",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ReGuilded/ReGuilded/issues"
  },
  "homepage": "https://github.com/ReGuilded/ReGuilded#readme",
  "dependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "fs-extra": "^8.1.0",
    "log4js": "^6.3.0",
    "yargs": "^7.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/electron": "^1.6.10",
    "rollup": "^2.60.1",
    "rollup-plugin-root-import": "^1.0.0",
    "typescript": "^4.5.2"
  }
}
